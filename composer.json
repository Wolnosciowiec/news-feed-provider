{
    "name": "wolnosciowiec/news-feed-provider",
    "license": "LGPLv3",
    "type": "project",
    "autoload": {
        "psr-4": {
            "AppBundle\\": "src/AppBundle",
            "RssSupportBundle\\": "src/RssSupportBundle",
            "ImageRepositoryBundle\\": "src/ImageRepositoryBundle",
            "WebProxyBundle\\": "src/WebProxyBundle",
            "Application\\Migrations\\": "app/DoctrineMigrations"
        },
        "classmap": [
            "app/NfpKernel.php",
            "app/AppCache.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        },
        "files": [
            "vendor/symfony/symfony/src/Symfony/Component/VarDumper/Resources/functions/dump.php"
        ]
    },
    "require": {
        "php": ">=7.1",
        "blocktrail/cryptojs-aes-php": "^0.1.0",
        "doctrine/doctrine-bundle": "^1.6",
        "doctrine/doctrine-migrations-bundle": "*",
        "doctrine/orm": "^2.5",
        "ezyang/htmlpurifier": "^4.9",
        "jms/serializer-bundle": "^2.4",
        "liip/monitor-bundle": "^2.5",
        "miniflux/picofeed": "@stable",
        "sensio/distribution-bundle": "^5.0.19",
        "sensio/framework-extra-bundle": "3.*",
        "symfony/monolog-bundle": "^3.1.0",
        "symfony/polyfill-apcu": "^1.0",
        "symfony/swiftmailer-bundle": "^2.3.10",
        "symfony/symfony": "3.*",
        "twig/twig": "^1.0||^2.0",
        "wolnosciowiec/file-repository-bundle": "dev-master",
        "zendframework/zenddiagnostics": "v1.1.0"
    },
    "require-dev": {
        "sensio/generator-bundle": "^3.0",
        "symfony/phpunit-bridge": "^3.0",
        "phpunit/phpunit": "^7.2"
    },
    "scripts": {
        "symfony-scripts": [
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ],
        "post-install-cmd": [
            "@symfony-scripts"
        ],
        "post-update-cmd": [
            "@symfony-scripts"
        ]
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-bin-dir": "bin",
        "symfony-var-dir": "var",
        "symfony-web-dir": "web",
        "symfony-tests-dir": "tests",
        "symfony-assets-install": "relative",
        "branch-alias": null
    }
}
